@model Dictionary<string, string>

@{
    ViewData["Title"] = "Hash Type Page";
}


<h1 class="display-4">List Type</h1>
<form asp-action="Add" method="post" enctype="application/x-www-form-urlencoded">
    <input type="text" name="key" />
    <input type="text" name="value" />
    <button type="submit">Save</button>
</form>


@foreach (var item in Model)
{
    <li>@item.Key @item.Value  <a asp-action="DeleteItem" asp-route-name="@item.Key">Delete</a></li>
}
